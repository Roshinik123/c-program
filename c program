#include <stdio.h>

int main() {
    // Write C code here
   int n,arr[n],count0=0,count1=0;
   scanf("%d",&n);
   for(int i=0;i<n;i++)
   {
       scanf("%d",&arr[i]);
   }
   for(int i=0;i<n;i++)
   {
       if(arr[i]==0)
       count0++;
       else
       count1++;
   }
   printf("%d\n%d",count0,count1);

    return 0;
}
