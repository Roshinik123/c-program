reverse tha array
#include <stdio.h>

int main() {
    // Write C code here
   int n;
   scanf("%d",&n);
   int arr[n];
   for(int i=0;i<n;i++)
   {
   scanf("%d",&arr[i]);
  }
   for(int i=n-1;i>=0;i--)
   {
       printf("%d",arr[i]);
}
    return 0;
}
count 0s and 1s
#include <stdio.h>

int main() {
    // Write C code here
   int n,arr[n],count0=0,count1=0;
   scanf("%d",&n);
   for(int i=0;i<n;i++)
   {
       scanf("%d",&arr[i]);
   }
   for(int i=0;i<n;i++)
   {
       if(arr[i]==0)
       count0++;
       else
       count1++;
   }
   printf("%d\n%d",count0,count1);

    return 0;
}
count less element than key element
#include <stdio.h>

int main() {
    // Write C code here
   int n,count=0,key;
   scanf("%d",&n);
   
   int arr[n];
   for(int i=0;i<n;i++)
   {
       scanf("%d ",&arr[i]);
  }
  scanf("%d",&key);
  
   for(int i=0;i<n;i++)
   {
       if(arr[i]<key)
       count++;
   }
   printf("%d",count);

}
